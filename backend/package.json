  "dependencies": {
    "axios": "^1.6.7",
    "bcryptjs": "^3.0.2",
    "bull": "^4.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-twitch": "^1.0.0",
    "redis": "^4.6.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.15.11",
    "@types/passport": "^1.0.12",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.3"
  }
