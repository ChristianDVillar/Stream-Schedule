{
  "name": "streamer-scheduler-web-backend",
  "version": "1.0.0",
  "private": true,
  "description": "Backend for Streamer Scheduler Web application",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "generate-certs": "node scripts/generate-certificates.js",
    "postinstall": "npm run generate-certs"
